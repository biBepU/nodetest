<!DOCTYPE html>
<html lang="en">
    <%- include('../components/head.ejs')%>
<body>

    <%- include( '../components/navbar.ejs') %>
   <div class="container">
    
    <% if(blogs.length){%>
        <% blogs.forEach(blogs=>{  %>
            <div class="bg-card">
                <h1><%= blogs.title  %></h1>
                <p><%= blogs.intro%></p>
            </div>
    
        <% }) %>
        <% }else{ %>
            <h3>No blogs found</h3>
       <% } %>
   </div>
     
</body>
</html>